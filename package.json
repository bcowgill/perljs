{
	"private": false,
	"name": "@bcowgill/perljs",
	"main": "index.js",
	"version": "0.3.3",
	"engines": {
		"node": ">=0.10.0"
	},
	"homepage": "https://github.com/bcowgill/perljs",
	"author": "Brent S.A. Cowgill <zardozcs@gmail.com> (http://github.com/bcowgill)",
	"description": "Perl for Javascript. Just some functions that a perl developer misses in Javascript.",
	"scripts": {
		"usage": "pnpm run; grunt --help",
		"check": "npm-check --color-always || true",
		"preprettier": "pnpm run prettier:json",
		"prettier": "pnpm run prettier:code",
		"prettier:code": "prettier --write Gruntfile.js $SRC $TESTS",
		"prettier:json": "prettier --write --parser json *.json .*.json",
		"rawprettier": "prettier --no-semi --single-quote --trailing-comma all --use-tabs --tab-width 4 --write $SRC $TESTS",
		"eslint": "eslint --cache --report-unused-disable-directives Gruntfile.js $SRC $TESTS",
		"eslint:fix": "pnpm run eslint -- --fix",
		"eslint:context": "pnpm run eslint -- --format codeframe",
		"htmllint": "htmllint $TESTS/**/*.html",
		"stylelint": "stylelint --cache --report-needless-disables --report-invalid-scope-disables --report-descriptionless-disables --formatter verbose scripts/**/*.css",
		"stylelint:fix": "stylelint --cache scripts/**/*.css --fix",
		"prelint:fix": "pnpm run stylelint:fix",
		"lint:fix": "pnpm run eslint:fix",
		"lint": "grunt lint",
		"test": "grunt",
		"test:tap": "mocha --reporter tap",
		"travis": "nyc --reporter lcovonly mocha",
		"watch": "grunt watch",
		"develop": "grunt all watch --force",
		"strip": "./scripts/strip.js",
		"prebuild": "pnpm run strip",
		"build": "grunt all",
		"doc": "pnpm run docs",
		"docs": "jsdoc --configure jsdoc.conf.json --destination doc --recurse test/*.js lib/perl.js Gruntfile.js README.md",
		"cover": "nyc grunt test",
		"coverage": "nyc grunt test",
		"coveralls": "nyc --check-coverage grunt test",
		"cover:testcss": "nyc grunt test",
		"postcover": "cp scripts/coverage/*.css doc/coverage",
		"postcoverage": "npm run postcover",
		"postcoveralls": "npm run postcover",
		"postcover:testcss": "cp scripts/coverage/*.css scripts/coverage/*.html doc/coverage",
		"cover-view": "chromium-browser doc/coverage/index.html",
		"coverage-view": "chromium-browser doc/coverage/index.html",
		"doc-view": "chromium-browser doc/index.html",
		"docs-view": "chromium-browser doc/index.html",
		"edit": "subl Gruntfile.js `find lib/ test/ -type f`",
		"postinstall": "husky install",
		"preversion": "./scripts/pre-version.sh",
		"version": "./scripts/version.sh",
		"postversion": "./scripts/post-version.sh",
		"NOTprepublishOnly": "pinst --disable",
		"prepublishOnly": "./scripts/npm-trace.sh prepublishOnly",
		"prepublish": "./scripts/pre-publish.sh",
		"publish": "./scripts/npm-trace.sh publish",
		"postpublish": "./scripts/npm-trace.sh postpublish",
		"NOTpostpublish": "pinst --enable"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bcowgill/perljs.git"
	},
	"keywords": ["perl"],
	"license": {
		"type": "Unlicense",
		"url": "http://unlicense.org/"
	},
	"bugs": {
		"url": "https://github.com/bcowgill/perljs/issues"
	},
	"devDependencies": {
		"abbrev": "^1.1.1",
		"async": "^3.2.0",
		"bower": "^1.8.12",
		"chai": "4.3.0",
		"coveralls": "^3.1.0",
		"escodegen": "^2.0.0",
		"eslint": "^7.20.0",
		"grunt": "^1.3.0",
		"grunt-contrib-clean": "^2.0.0",
		"grunt-contrib-concat": "^1.0.1",
		"grunt-contrib-connect": "^3.0.0",
		"grunt-contrib-jshint": "^3.0.0",
		"grunt-contrib-uglify": "^5.0.0",
		"grunt-contrib-watch": "^1.1.0",
		"grunt-eslint": "^23.0.0",
		"grunt-htmllint": "^0.3.0",
		"grunt-jsdoc": "^2.4.1",
		"grunt-mocha-chai-sinon": "0.0.9",
		"grunt-mocha-istanbul": "^5.0.1",
		"grunt-prettier": "^2.1.0",
		"grunt-stylelint": "^0.16.0",
		"handlebars": "^4.7.7",
		"htmllint": "0.7.2",
		"htmllint-cli": "^0.0.7",
		"husky": "^5.1.1",
		"istanbul": "^0.4.3",
		"jsdoc": "^3.6.6",
		"mkdirp": "^1.0.4",
		"mocha": "8.3.0",
		"nopt": "^5.0.0",
		"npm-check": "^5.9.2",
		"nyc": "^15.1.0",
		"pinst": "^2.1.6",
		"prettier": "^2.2.1",
		"stylelint": "^13.11.0",
		"stylelint-config-standard": "^20.0.0",
		"uncommentify": "^0.1.1"
	}
}
